                <div class="tilt-hitbox"></div>
            </div>
        </div>
        <div class="zoom-wrapper">
            <div class="zoom-content">
                <section class="hero">
                    <div class="hero-wrapper">
                        <div class="hero-header">
                            <!-- Title moved to global scope for z-index layering -->
                        </div>

                        <!-- Spacer to push content down below fixed title -->
                        <div class="hero-spacer"></div>

                        <div class="hero-main-content">
                            <!-- Placeholder to keep grid layout -->
                            <div class="hero-visual-placeholder"></div>

                            <div class="hero-content-right">
                                <img src="{{ url_for('static', filename='assets/images/resources/postcript.svg') }}"
                                    class="pd-text-img" alt="P.D:">
                                <p class="hero-description">
                                    POSDATA es un estudio de diseño y desarrollo web. Seguimos cada proyecto desde
                                    la idea
                                    inicial hasta su publicación, integrando diseño, código y puesta online — todo
                                    en un mismo
                                    proceso. Acompañamos a marcas y proyectos que valoran la calidad, construyendo
                                    sitios
                                    estéticos, claros y bien pensados.
                                </p>
                            </div>
                        </div>

                        <div class="hero-footer-strip">
                            <img src="{{ url_for('static', filename='assets/images/resources/edition-01.svg') }}"
                                class="footer-edition" alt="Edition 01">
                            <span class="copyright">© 2026 POSDATA</span>
                            <img src="{{ url_for('static', filename='assets/images/resources/signature.svg') }}"
                                class="footer-signature" alt="Signature with initials J and G">
                        </div>
                    </div>
                </section>

