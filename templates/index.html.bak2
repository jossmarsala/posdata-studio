        <!-- Moved out to separate z-index context from Hero Text -->
        <div class="hero-visual">
            <div class="image-container" id="imageContainer">
                <div class="background-stack">
                    <div class="bg-card"><img
                            src="{{ url_for('static', filename='assets/images/mountains-polaroid.webp') }}"
                            alt="Landscape 1"></div>
                    <div class="bg-card"><img
                            src="{{ url_for('static', filename='assets/images/flowers-polaroid.webp') }}"
                            alt="Landscape 2"></div>
                    <div class="bg-card"><img
                            src="{{ url_for('static', filename='assets/images/bouquette-polaroid.webp') }}"
                            alt="Landscape 3"></div>
                </div>

                <div class="foreground-stack">
                    <div class="fg-card active"><img
                            src="{{ url_for('static', filename='assets/images/mountains-polaroid.webp') }}"
                            alt="Landscape 1"></div>
                    <div class="fg-card"><img
                            src="{{ url_for('static', filename='assets/images/flowers-polaroid.webp') }}"
                            alt="Landscape 2"></div>
                    <div class="fg-card"><img
                            src="{{ url_for('static', filename='assets/images/bouquette-polaroid.webp') }}"
